{
  "manifest_version": 2,
  "name": "Googol Modifier",
  "short_name": "Googol Mod",
  "version": "1.2",
  "description": "Customize Google's logo to whatever you want",
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://*.google.com/*"]
  }],
  "browser_action": {
  	"default_popup": "popup.html",
    "default_title": "Googol Modifer"
  },
  "permissions": [
   	"storage"
   ]
}